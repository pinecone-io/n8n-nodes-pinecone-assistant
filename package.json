{
    "name": "@pinecone/n8n-nodes-pinecone-assistant",
    "version": "0.1.0",
    "description": "A Pinecone Assistant node for n8n",
    "license": "MIT",
    "homepage": "",
    "keywords": [
        "n8n-community-node-package",
        "n8n",
        "pinecone",
        "pinecone assistant",
        "rag",
        "retrieval-augmented generation"
    ],
    "author": {
        "name": "Jenna Pederson",
        "url": "https://github.com/jennapederson"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/pinecone-io/n8n-nodes-pinecone-assistant.git"
    },
    "scripts": {
        "build": "n8n-node build",
        "build:watch": "tsc --watch",
        "dev": "n8n-node dev",
        "lint": "n8n-node lint",
        "lint:fix": "n8n-node lint --fix",
        "release": "n8n-node release",
        "prepublishOnly": "n8n-node prerelease",
        "test": "jest",
        "test:watch": "jest --watch"
    },
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "strict": true,
        "credentials": [
            "dist/credentials/PineconeApi.credentials.js"
        ],
        "nodes": [
            "dist/nodes/PineconeAssistant/PineconeAssistant.node.js"
        ]
    },
    "devDependencies": {
        "@n8n/node-cli": "*",
        "@types/jest": "^29.5.12",
        "@types/node": "^24.10.0",
        "eslint": "9.32.0",
        "jest": "^29.7.0",
        "prettier": "3.6.2",
        "release-it": "^19.0.4",
        "ts-jest": "^29.1.2",
        "typescript": "5.9.2"
    },
    "peerDependencies": {
        "n8n-workflow": "*"
    }
}
